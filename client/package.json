{
  "name": "back-office",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "NODE_ICU_DATA=\"$(pwd)/node_modules/full-icu\" vue-cli-service test:e2e",
    "lint": "run-p lint:*",
    "build:admin": "vue-cli-service build",
    "build:candidat": "vue-cli-service build",
    "cypress": "cypress run",
    "format": "run-s lint:js",
    "lint:js": "vue-cli-service lint",
    "lint:style": "vue-cli-service lint:style",
    "prebuild:admin": "cp .env.admin .env",
    "prebuild:candidat": "cp .env.candidat .env",
    "sb": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "start": "npm run serve",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "test": "NODE_ICU_DATA=\"$(pwd)/node_modules/full-icu\" npm run -s test:unit",
    "test:e2e-local": "npm run test:e2e -- --env build=1 --config numTestsKeptInMemory=3",
    "test:e2e-local-headless": "npm run test:e2e -- --env build=1 --config numTestsKeptInMemory=3 --headless",
    "test:watch": "NODE_ICU_DATA=\"$(pwd)/node_modules/full-icu\" npm run -s test:unit -- --watch"
  },
  "dependencies": {
    "ag-grid-community": "^23.2.1",
    "ag-grid-vue": "^23.2.1",
    "chart.js": "^2.9.3",
    "core-js": "^3.6.5",
    "delay": "^4.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "html-loader": "^1.1.0",
    "intl": "^1.2.5",
    "lodash-es": "^4.17.15",
    "luxon": "^1.24.1",
    "normalize.css": "^8.0.1",
    "p-debounce": "^2.1.0",
    "register-service-worker": "^1.7.1",
    "v-clipboard": "^2.2.3",
    "vue": "^2.6.11",
    "vue-chartjs": "^3.5.0",
    "vue-class-component": "^7.2.3",
    "vue-intl": "^3.1.0",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.3.4",
    "vue-scrollto": "^2.18.1",
    "vuetify": "^2.3.3",
    "vuex": "^3.5.1",
    "whatwg-fetch": "^3.1.0"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "^1.1.2",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addon-notes": "5.3.19",
    "@storybook/addon-viewport": "5.3.19",
    "@storybook/vue": "5.3.19",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-e2e-cypress": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-plugin-pwa": "^4.4.6",
    "@vue/cli-plugin-router": "^4.4.6",
    "@vue/cli-plugin-unit-jest": "^4.4.6",
    "@vue/cli-plugin-vuex": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "cypress-file-upload": "^4.0.7",
    "eslint": "^7.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^6.2.2",
    "full-icu": "^1.3.1",
    "lint-staged": "^10.2.11",
    "material-design-icons-iconfont": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-current-selector": "0.0.3",
    "postcss-nested": "^4.2.1",
    "postcss-nested-ancestors": "^2.0.0",
    "postcss-nesting": "^7.0.1",
    "postcss-preset-env": "^6.7.0",
    "quoted-printable": "^1.0.1",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.26.9",
    "sass-loader": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "svg-inline-loader": "^0.8.2",
    "typeface-roboto": "0.0.75",
    "vue-cli-plugin-storybook": "~1.2.2",
    "vue-cli-plugin-vuetify": "^2.0.6",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.5.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:vue/recommended",
      "@vue/standard"
    ],
    "rules": {
      "jsx-quotes": [
        2,
        "prefer-double"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "vue/no-v-html": 0
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-preset-env": {},
      "postcss-nested-ancestors": {},
      "postcss-nested": {},
      "postcss-current-selector": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/src/**/*.spec.js|**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "transformIgnorePatterns": [
      "/node_modules/(?!lodash-es)"
    ]
  }
}
